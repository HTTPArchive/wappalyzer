{
  "devDependencies": {
    "@google-cloud/bigquery": "7.9.3",
    "@google-cloud/storage": "7.16.0",
    "@prantlf/jsonlint": "16.0.0",
    "jest": "29.7.0",
    "puppeteer": "24.6.0",
    "standard": "17.1.2",
    "terminal-overwrite": "2.0.1",
    "webpagetest": "github:HTTPArchive/WebPageTest.api-nodejs"
  },
  "scripts": {
    "lint": "standard && jsonlint -jsV ./schema.json --trim-trailing-commas --enforce-double-quotes ./src/technologies/ && jsonlint -js --trim-trailing-commas --enforce-double-quotes ./src/categories.json",
    "lint:fix": "standard --fix && jsonlint -isV ./schema.json --trim-trailing-commas --enforce-double-quotes ./src/technologies/ && jsonlint -is --trim-trailing-commas --enforce-double-quotes ./src/categories.json",
    "validate": "node ./scripts/validate.js",
    "test": "jest",
    "tech_upload": "node ./scripts/tech_upload.js",
    "convert": "node ./scripts/convert.js",
    "build": "npm run lint && npm run validate && npm run convert"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "github-actions",
        {
          "silent": false
        }
      ]
    ]
  }
}
